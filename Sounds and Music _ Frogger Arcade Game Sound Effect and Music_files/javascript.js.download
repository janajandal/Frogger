// Menu Expander / Toggle Visibility 
function toggle_menu(id) {
    var e = document.getElementById(id);
    if (e.classList.contains("showing"))
      e.classList.remove("showing");
    else
      e.classList.add("showing");
}

// Select Box Option, Open in Window
function download(d) {
		if (d == '-- HD 16:9 --' || d == '-- Standard 4:3 --' || d == '-- Standard 5:4 --' || d == '-- Widescreen 16:10 --' || d == '-- Widescreen 5:3 --') return;
		window.open('images/wallpaper/' + d,'_blank');
}
